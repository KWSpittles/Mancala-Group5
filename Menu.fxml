<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scenePane" minHeight="720.0" minWidth="720.0" prefHeight="720.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuController">
   <children>
      <AnchorPane fx:id="menuPane" layoutX="50.0" layoutY="96.0" prefHeight="529.0" prefWidth="545.0">
         <children>
            <Button fx:id="loginButton" layoutX="185.0" layoutY="441.0" mnemonicParsing="false" onAction="#logout" prefHeight="65.0" prefWidth="118.0" text="Logout">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Label fx:id="title" layoutX="182.0" layoutY="14.0" prefHeight="42.0" prefWidth="164.0" text="Welcome to Mancala!" />
            <Label fx:id="loginmessage" layoutX="79.0" layoutY="340.0" prefHeight="65.0" prefWidth="371.0" wrapText="true" />
            <Button layoutX="182.0" layoutY="126.0" mnemonicParsing="false" onAction="#switchToGame" prefHeight="65.0" prefWidth="125.0" text="Play Game" />
            <Button layoutX="185.0" layoutY="220.0" mnemonicParsing="false" onAction="#switchToLeaderboard" prefHeight="65.0" prefWidth="125.0" stylesheets="@sample.css" text="Leader Board" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
